#include "tennislog.h"

namespace sdds {
TennisMatch::operator bool() const { return false; }

std::ostream &operator<<(std::ostream &os, const TennisMatch &match) {
  if (!match) {
    os << "Empty Match";
  }
}
} // namespace sdds
